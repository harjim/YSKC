<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yskc.ms.dao.rs.CompanyHolidayDao">
    <select id="getMonthHoliday" resultType="com.yskc.ms.models.company.CompanyHolidayModel">
        select id,`month`,holidays from c_holiday where companyId = #{companyId} and `month` = #{month}
    </select>
</mapper>