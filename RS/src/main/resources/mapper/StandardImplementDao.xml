<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yskc.rs.dao.StandardImplementDao">
    <select id="getInfo" resultType="com.yskc.rs.entity.StandardImplementEntity">
        select *
    from p_standard_implement
    where `year`=#{year} and companyId=#{companyId}
    </select>
</mapper>